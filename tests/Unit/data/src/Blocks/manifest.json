				"slug": "black",
				"color": "#000000"
			},
			{
				"name": "Light",
				"slug": "light",
				"color": "#CCCCCC"
			},
			{
				"name": "White",
				"slug": "white",
				"color": "#FFFFFF"
			}
		],
		"gradients": [
			{
				"name": "Black",
				"slug": "black",
				"gradient": "#000000"
			}
		],
		"fontSizes": [
			{
				"name": "Normal",
				"slug": "normal",
				"size": "20"
			}
		]
	}
