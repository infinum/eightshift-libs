{
	"namespace": "eightshift-boilerplate",
	"background": "#F6F8F8",
	"foreground": "#0D3636",
	"config": {
		"outputCssGlobally": true,
		"outputCssOptimize": true,
		"outputCssSelectorName": "esCssVariablesTest",
		"outputCssGloballyAdditionalStyles": [
			":root {--es-loader-opacity: 1;}"
		]
	},
	"globalVariables": {
		"customBlocksName": "eightshift-block",
		"maxCols": 12,
		"breakpoints": {
			"mobile": 767,
			"tablet": 991,
			"desktop": 1199,
			"large": 1200
		},
		"containers": {
			"default": "66em"
		},
		"gutters": {
			"none": "0",
			"default": "1.25em",
			"big": "2.5em"
		},
		"sectionSpacing": {
